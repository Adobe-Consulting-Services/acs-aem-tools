    jcr:primaryType="nt:unstructured">
    <items jcr:primaryType="nt:unstructured">
        <headerbar jcr:primaryType="nt:unstructured">
            <items jcr:primaryType="nt:unstructured">
                <url
                    granite:class="editor-GlobalBar-item qr-code-url qrcode-hidden"
